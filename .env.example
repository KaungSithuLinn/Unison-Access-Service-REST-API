# Unison API Configuration
# Copy this file to .env and update with your actual values
# NEVER commit .env to version control

# API Configuration
UNISON_API_BASE_URL=https://your-api-server.com:9001/Unison.AccessService
UNISON_API_TOKEN=your-secure-token-here

# Security Settings
UNISON_API_VERIFY_SSL=true
UNISON_API_TIMEOUT=30

# File Upload Limits (in bytes)
UNISON_API_MAX_FILE_SIZE=5242880

# Logging Configuration
LOG_LEVEL=INFO
