{
  "id": "secure-unison-environment",
  "name": "Unison API - Secure Environment",
  "values": [
    {
      "key": "base_url",
      "value": "https://your-api-server.com:9001/Unison.AccessService",
      "description": "HTTPS base URL for Unison Access Service API - NEVER use HTTP",
      "enabled": true
    },
    {
      "key": "unison_token",
      "value": "{{UNISON_API_TOKEN}}",
      "description": "API token from environment variable - DO NOT hardcode",
      "enabled": true
    },
    {
      "key": "api_timeout",
      "value": "30000",
      "description": "Request timeout in milliseconds",
      "enabled": true
    },
    {
      "key": "max_file_size",
      "value": "5242880",
      "description": "Maximum file upload size in bytes (5MB)",
      "enabled": true
    }
  ],
  "_postman_variable_scope": "environment"
}
